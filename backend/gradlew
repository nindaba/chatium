#!/bin/sh

##############################################################################
#
#   Gradle start up script for POSIX generated by Gradle.
#
##############################################################################

# Use gradle command if gradlew is not fully set up
if [ ! -f "$(dirname "$0")/gradle/wrapper/gradle-wrapper.jar" ]; then
    echo "Gradle wrapper jar not found. Using system gradle..."
    exec gradle "$@"
fi

# Standard Gradle wrapper script would go here
exec gradle "$@"
